<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onboardingFragment">

    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.praktikum.abstreetfood_management.ui.OnboardingFragment"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" >
        <action
            android:id="@+id/action_onboardingFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.praktikum.abstreetfood_management.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true"/> <action
        android:id="@+id/action_loginFragment_to_registerFragment"
        app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.praktikum.abstreetfood_management.ui.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.praktikum.abstreetfood_management.ui.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action android:id="@+id/action_homeFragment_to_dashboardFragment" app:destination="@id/dashboardFragment" />
        <action android:id="@+id/action_homeFragment_to_inventarisFragment" app:destination="@id/inventarisFragment" />
        <action android:id="@+id/action_homeFragment_to_transaksiFragment" app:destination="@id/transaksiFragment" />
        <action android:id="@+id/action_homeFragment_to_pengaturanFragment" app:destination="@id/pengaturanFragment" />
        <action android:id="@+id/action_homeFragment_to_analisisFragment" app:destination="@id/analisisFragment" />
        <action android:id="@+id/action_homeFragment_to_profileFragment" app:destination="@id/profileFragment" />
    </fragment>

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.praktikum.abstreetfood_management.ui.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/inventarisFragment"
        android:name="com.praktikum.abstreetfood_management.ui.InventarisFragment"
        android:label="fragment_inventaris"
        tools:layout="@layout/fragment_inventaris" />
    <fragment
        android:id="@+id/transaksiFragment"
        android:name="com.praktikum.abstreetfood_management.ui.TransaksiFragment"
        android:label="fragment_transaksi"
        tools:layout="@layout/fragment_transaksi" />
    <fragment
        android:id="@+id/pengaturanFragment"
        android:name="com.praktikum.abstreetfood_management.ui.PengaturanFragment"
        android:label="fragment_pengaturan"
        tools:layout="@layout/fragment_pengaturan" />
    <fragment
        android:id="@+id/analisisFragment"
        android:name="com.praktikum.abstreetfood_management.ui.AnalisisFragment"
        android:label="fragment_analisis"
        tools:layout="@layout/fragment_analisis" />
<!--    <fragment-->
<!--        android:id="@+id/profileFragment"-->
<!--        android:name="com.praktikum.abstreetfood_management.ui.ProfileFragment"-->
<!--        android:label="fragment_profile"-->
<!--        tools:layout="@layout/fragment_profile" />-->
    <fragment
        android:id="@+id/laporanFragment"
        android:name="com.praktikum.abstreetfood_management.ui.LaporanFragment"
        android:label="fragment_laporan"
        tools:layout="@layout/fragment_laporan" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.praktikum.abstreetfood_management.ui.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>

    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.praktikum.abstreetfood_management.ui.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />



</navigation>